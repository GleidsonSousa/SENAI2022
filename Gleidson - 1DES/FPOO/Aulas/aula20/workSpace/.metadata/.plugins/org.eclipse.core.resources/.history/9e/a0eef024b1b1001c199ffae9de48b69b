package modelo;

public class Animal {

	private String especie;
	private String nome;
	private float peso;
	private String raca;

	
	
	public Animal () {		
	}

	public Animal (String especie, String nome, float peso, String raca ) {
		super();
		this.especie = especie;
		this.nome = nome;
		this.peso = peso;
		this.raca = raca;
		
	}
}


