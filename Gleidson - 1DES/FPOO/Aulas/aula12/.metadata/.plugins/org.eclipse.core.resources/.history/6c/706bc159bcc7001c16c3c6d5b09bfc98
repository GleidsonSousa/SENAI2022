package modelo;

public class Agenda01 {
	public int id;
	public String nome;
	public int idade;
	public String telefone;
	Agenda01(){}
	Agenda01(int id, String nome, int idade, String telefone){
		this.id = id;
		this.nome = nome;
		this.idade = idade;
		this.telefone = telefone;
	}

	public String tabular() {
		return id+"\t"+nome+"\t"+idade+"\t"+telefone;
	}
	
	}
