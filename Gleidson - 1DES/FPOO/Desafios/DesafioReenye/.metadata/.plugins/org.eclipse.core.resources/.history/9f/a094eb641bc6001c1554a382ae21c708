package modelo;

import java.util.ArrayList;

public class PlanilhaPessoas {

	private int v1;
	private int v2;

	// Construtor
	public PlanilhaPessoas(String linha) {
		// TODO Auto-generated constructor stub
	}

	public void Planilha(String entrada) {
		v1 = Integer.parseInt(entrada.split(";")[0]);
		v2 = Integer.parseInt(entrada.split(";")[1]);
	}

	// Métodos para Processamento
	public int soma() {
		return v1 + v2;
	}
}

