package exemploclassicos;

public class Acumula {

	public static void main(String[] args) {
		System.out.println("O algoritimo de acumulção simples");
		System.out.println("simples,retornou=");
		System.out.println(simples());
	}

	
	public static int simples() {
		int x =0;
		for (int i = 0; i <10; i++);
		x+=i;
		return x;

		
}
	
	
}
