package exvetor;
import java.util.Scanner;
public class Lista2_1 {
	public static Scanner input;
	public static void main(String[] args) {
		
		int[] vetor = new int [5];
		vetor	[0] = 2;
		vetor	[1] = 4;
		vetor	[2] = 5;
		vetor	[3] = 6;
		vetor	[4] = 8;
		for(int i = 0; i<vetor.length ; i++) {
			System.out.println("Valores " + vetor[i] + ": ");
			vetor[i] = input.nextInt();
		
			
		
		
		
		
		
	}

}}
