package revisao;

import java.util.Scanner;
public class revisao {

	public static void main(String[] args) {
		
       // declaração de matriz
		int[][] m = new int[5][5]; 
		 
		 Random r = new Random();
		 
		   //pecorrer as colunas 
		 for(int i = 0; < 5; i++) {
			// percorrer as colunas 
		for(int c = 0; i< 4; i++) {
			m[i][c] = r.nextInt(100);
			System.out.println(m [i][c]+ "/t");
				
			}
			
		}

	}

}
